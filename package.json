{
  "license": "Apache license 2.0",
  "description": "Aether Code is a web-based IDE that allows you to write, run, and debug code in your browser. It supports multiple programming languages and provides a collaborative environment for developers.",
  "author": "Sky Genesis Enterprise",
  "name": "react-vite-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "contact": {
    "email": "contact@skygenesisenterprise.com",
    "url": "https://skygenesisenterprise.com"
  },
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build --outDir dist",
    "lint": "bunx biome lint --write && bunx tsc --noEmit",
    "format": "bunx biome format --write",
    "preview": "vite preview",
    "electron:dev": "cd electron && npm install && npm start",
    "electron:build": "cd electron && npm install && electron-builder",
    "docker:build": "docker build -t aether-code .",
    "docker:run": "docker run -p 3000:3000 --env-file .env aether-code",
    "docker:compose": "docker-compose up --build",
    "api:dev": "nodemon app/api/server.js", 
    "api:lint": "eslint app/api --ext .js,.ts",
    "api:test": "jest --config app/api/jest.config.js",
    "api:build": "tsc -p app/api/tsconfig.json",
    "api:start": "node app/api/server.js"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.5",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-slot": "^1.2.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.6.3",
    "lucide-react": "^0.487.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.18.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.3.4"
  }
}
